//////////////////////////////////////////

function computeSumBetween(num1, num2) {
  // your code here
    var count = 0;
    for(var x = num1; x < num2 + 1; x++){
      count += x;
    }
    return(count - num2);
  }
  
//////////////////////////////////////////
  
var output = computeSumBetween(2, 5);
console.log(output); // --> 9

